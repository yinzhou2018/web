<template id="app_configuration_panel" type="text/template">
  <script type='text/javascript' src='js/app_configuration_panel.js'></script>
  <div style='padding-left: 20px'>
    <div style='margin-bottom:10px;margin-top:10px'>
      应用名: <input id='app_id' class="easyui-textbox" style="width:200px">
    </div>
    <div style="margin-bottom:10px">
        <input id='app_desc' class="easyui-textbox" label="应用描述:" labelPosition="top" multiline="true" style="width:600px;height:120px">
    </div>
    <div style="margin-bottom:10px">
        <input id='app_conf' class="easyui-textbox" label="应用配置:" labelPosition="top" multiline="true" style="width:600px;height:500px">
    </div>
    <a href="#" class="easyui-linkbutton" id="app_conf_commit" style="width:80px; margin-right:10px">提交</a>
    <a href="#" class="easyui-linkbutton" id="app_conf_cancel" style="width:80px">取消</a>
  </div>
 </template>